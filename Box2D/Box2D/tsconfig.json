{
  "compilerOptions": {
    "target": "ES2016",
    "module": "system",
    "preserveConstEnums": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitUseStrict": true,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false
  },
  "include": [
    "**/*.ts"
  ]
}
